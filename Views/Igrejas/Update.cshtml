@model Igreja

<div>
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Update">
                @Html.LabelFor(x=> x.SetorADM, new {@class = "form-label"})
                @Html.TextBoxFor(x => x.SetorADM, new{@class = "form-control"})

                @Html.LabelFor(x=> x.Bairro, new {@class = "form-label"})
                @Html.TextBoxFor(x => x.Bairro, new{@class = "form-control"})

                @Html.LabelFor(x=> x.Nome, new {@class = "form-label"})
                @Html.TextBoxFor(x => x.Nome, new{@class = "form-control"})

                @Html.LabelFor(x=> x.Localidade, new {@class = "form-label"})
                @Html.TextBoxFor(x => x.Localidade, new{@class = "form-control"})

                @Html.HiddenFor(model => model.Id)
                <div class="form-group">
                    <input type="submit" value="Atualizar" class="btn btn-primary" </input>
                    <input type="submit" value="Cancelar" class="btn btn-danger" </input>
                </div>
            </form>
        </div>
    </div>
</div>
